    "hono": "^4.8.10",
    "jotai": "^2.12.5",
    "next": "14.2.0",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "recharts": "^3.1.0",
    "swr": "^2.3.4"
  },
